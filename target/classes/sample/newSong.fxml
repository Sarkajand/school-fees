<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<DialogPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="sample.NewSongController"
            prefHeight="400.0" prefWidth="600.0">
    <content>
        <GridPane vgap="10" hgap="10">
            <Label text="Artist Name" GridPane.rowIndex="0" GridPane.columnIndex="0"/>
            <TextField fx:id="artistNameField" GridPane.rowIndex="0" GridPane.columnIndex="1"/>
            <Label text="Album Name" GridPane.rowIndex="1" GridPane.columnIndex="0"/>
            <TextField fx:id="albumNameField" GridPane.rowIndex="1" GridPane.columnIndex="1"/>
            <Label text="Title" GridPane.rowIndex="2" GridPane.columnIndex="0"/>
            <TextField fx:id="titleField" GridPane.rowIndex="2" GridPane.columnIndex="1"/>
            <Label text="Track" GridPane.rowIndex="3" GridPane.columnIndex="0"/>
            <TextField fx:id="trackField" GridPane.rowIndex="3" GridPane.columnIndex="1"/>
        </GridPane>
    </content>
</DialogPane>
